<?xml version="1.0" encoding="UTF-8" ?>
<ehcache>
    <!-- 定义默认的缓存区，如果在未指定缓存区时，默认使用该缓存区 -->
    <defaultCache maxElementsInMemory="500" eternal="true"
        overflowToDisk="false" memoryStoreEvictionPolicy="LRU">
    </defaultCache>
    <!-- 定义名字为"staticCtx"的缓存区 -->
    <cache name="staticCtx" maxElementsInMemory="500" eternal="true"
        overflowToDisk="false" memoryStoreEvictionPolicy="LRU" />
    <!-- 定义名字为"wx.share"的缓存区 -->
    <cache name="wx.share" maxElementsInMemory="500" eternal="true"
        overflowToDisk="false" memoryStoreEvictionPolicy="LRU" />
</ehcache>